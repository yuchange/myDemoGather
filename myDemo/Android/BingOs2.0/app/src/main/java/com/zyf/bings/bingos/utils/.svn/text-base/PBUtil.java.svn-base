package com.zyf.bings.bingos.utils;

import android.content.Context;

import com.zyf.bings.bingos.R;
import com.zyf.bings.bingos_libnet.progressDialog.MyProgressDialog;


/**
 * Created by Jin on 2016/4/1.
 */
public class PBUtil {


    public static MyProgressDialog getPD(Context context) {
        if (context == null) {
            return null;
        }


        MyProgressDialog sProgressDialog = new MyProgressDialog(context, R.style.MyProgressDialog, R.layout.dialog_progress);
        sProgressDialog.setCanceledOnTouchOutside(false);
        return sProgressDialog;

    }
}
