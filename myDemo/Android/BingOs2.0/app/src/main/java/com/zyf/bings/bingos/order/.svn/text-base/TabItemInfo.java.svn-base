package com.zyf.bings.bingos.order;


import android.support.v4.app.Fragment;

/**
 * Created wangshiqi sd on 2017/9/21.
 */

class TabItemInfo {
    private Fragment fragmentClass;
    private String  nameResource;
    private int numResource;

    public TabItemInfo(Fragment fragmentClass,  String nameResource,
                       int numResource) {
        this.fragmentClass = fragmentClass;
        this.nameResource = nameResource;
        this.numResource = numResource;
    }

    public Fragment getFragmentClass() {
        return fragmentClass;
    }

    public String  getNameResource() {
        return nameResource;
    }

    public int  getNumResource() {
        return numResource;
    }
}

