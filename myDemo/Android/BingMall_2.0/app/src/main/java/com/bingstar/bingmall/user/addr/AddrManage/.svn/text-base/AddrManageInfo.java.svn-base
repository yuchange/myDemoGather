package com.bingstar.bingmall.user.addr.AddrManage;

/**
 * Created by John on 2017/3/3.
 */

public class AddrManageInfo {
    private String name;
    private String phone;
    private String place;
    private String flug;

    public AddrManageInfo(String name, String phone, String place, String flug) {
        this.name = name;
        this.phone = phone;
        this.place = place;
        this.flug = flug;
    }

    public String getFlug() {
        return flug;
    }

    public void setFlug(String flug) {
        this.flug = flug;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getPhone() {
        return phone;
    }

    public void setPhone(String phone) {
        this.phone = phone;
    }

    public String getPlace() {
        return place;
    }

    public void setPlace(String place) {
        this.place = place;
    }
}
